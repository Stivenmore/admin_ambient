    "version": "0.2.0",
    "configurations": [
        {
            "name": "admin_ambient (debug mode)",
            "request": "launch",
            "type": "dart",
            "deviceId": "chrome",
            "flutterMode": "debug",
            "args": [
                "-d --web-renderer html"
            ]
        },
        {
            "name": "admin_ambient (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "web --web-renderer html --release"
            ]
        }
    ]
}